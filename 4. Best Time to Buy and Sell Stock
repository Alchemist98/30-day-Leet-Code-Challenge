class Solution(object):
    def maxProfit(self, prices):
        maxSum = 0
        diff = 0
        for i in range(0,len(prices)-1):
            for j in range(i+1,len(prices)):
                if prices[j] > prices[i]:
                    diff = prices[j] - prices[i]
                    maxSum = max(diff,maxSum)          
                
        return maxSum
